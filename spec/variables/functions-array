{{ as|remove("b", "c") }}-{{ a|remove(["b", "c", "d"], "e", "f")|implode("") }}
{% if a|contains("b") %}yes{% else %}no{% endif %}
{% if as|contains("c") %}yes{% endif %}{% if as|contains("x") %}no{% endif %}
