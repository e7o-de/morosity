------------------------------------------------------------------------------
Switchs can be nested.
------------------------------------------------------------------------------
{% switch a %}
{% case 1 %}x1{% switch b %}{% case 13 %}w1{% case * %}w2{% endswitch %}
{% case 3 %}c1{% switch b %}{% case 13 %}c2{% case * %}w3{% endswitch %}
{% case 5 %}x2{% switch b %}{% case 13 %}w4{% case * %}w5{% endswitch %}
{% endswitch %}
------------------------------------------------------------------------------
{"a": 3, "b": 13}
------------------------------------------------------------------------------
c1c2
